func firstOnHundredThousandPrimes with(
    bool.100000(primes),
    int(i, j, p, n),
    bool(boolBuffer, boolBuffer2)
){
    i = 0
    n = 100000
    while i < n{
        ASET(primes, i, true)
        i = i + 1
    }
    p = 2
    while p < n{
        AGET(boolBuffer, primes, p)
        j = p * p
        if boolBuffer{
            while j < n && j > 0{
                ASET(primes, j, false)
                j = j + p
            }
        }
        p = p + 1
    }
    i = 0
    j = 0
    while i < n{
        AGET(boolBuffer, primes, i)
        if boolBuffer{
            j = j + 1
        }
        i = i + 1
    }
    PRINT(j)
}

func main{
    CALL(firstOnHundredThousandPrimes)
}