func fib{
    REFA(a, 2, 0)
    REFA(b, 2, 0)
    REFA(c, 2, 0)
    REFA(n, 2, 0)
    REFA(true, 0, 0)
    REFA(grtBuffer, 0, 0)
    REFA(nBuffer, 2, 0)
    true = 1
    REFA(zeroBuffer, 2, 0)
    zeroBuffer = 0
    REFA(oneBuffer, 2, 0)
    oneBuffer = 1
    REFA(twoBuffer, 2, 0)
    twoBuffer = 2
    a = 0
    b = 1
    c = 1
    GARG(n, 0)
    LABEL(loopLabel)
    a = b
    b = c
    c = a + b
    n = n - oneBuffer
    nBuffer = n - twoBuffer
    grtBuffer = nBuffer > zeroBuffer
    PRINT(c)
    BEQ(grtBuffer, true, loopLabel)
}
func main{
    REFA(n, 2, 0)
    n = 30
    PARG(n)
    CALL(fib)
}