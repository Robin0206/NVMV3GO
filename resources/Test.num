
func fib{
    REFA(a, 3, 0)
    REFA(b, 3, 0)
    REFA(c, 3, 0)
    REFA(n, 2, 0)
    REFA(k, 0, 0)
    a = 0.0
    b = 1.0
    c = 1.0
    GARG(n, 0)
    GARG(k, 1)
    if k{
        while n - 2 > 0{
                a = b
                b = c
                c = (a + b) * 2.0
                n = n - 1
                PRINT(c)
       }
    }
    else{
        while n - 2 > 0{
            a = b
            b = c
            c = a + b
            n = n - 1
            PRINT(c)
        }
    }
}

func main{
    REFA(n, 2, 0)
    REFA(k, 0, 0)
    n = 30
    k = true
    PARG(n)
    PARG(k)
    CALL(fib)
}