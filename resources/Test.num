}

func fibRekuTest(int n) with(
    int(leftResult, rightResult)
){
    if n < 2{
        return n
    }else{
        rightResult = fibRekuTest(n - 1)
        leftResult = fibRekuTest(n - 2)
        return leftResult + rightResult
    }
}

func main with(
    int(n, result),
    bool.100000(primes)
){
    n = 100000
    wolfram()
    primeTest(primes, n)
    result = 0
    for i from 0 to n{
        if primes.i{
            result = result + 1
        }
    }
    n = 6
    PRINT(fibRekuTest(fibRekuTest(n)))
    PRINT(result)
}