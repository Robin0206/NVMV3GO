
func fib(int n, bool k){
    REFA(a, 3, 0)
    REFA(b, 3, 0)
    REFA(c, 3, 0)
    a = 0.0
    b = 1.0
    c = 1.0
    if k{
        while n - 2 > 0{
                a = b
                b = c
                c = (a + b) * 2.0
                n = n - 1
       }
    }
    else{
        while n - 2 > 0{
            a = b
            b = c
            c = a + b
            n = n - 1
        }
    }
    RETURN(c)
}

func main{
    REFA(n, 2, 0)
    REFA(k, 0, 0)
    REFA(result, 3, 0)
    n = 30
    k = false
    PARG(n)
    PARG(k)
    CALL(fib)
    RETG(result)
    PRINT(result)
}